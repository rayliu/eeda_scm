	<queryString>
		<![CDATA[SELECT
	wo.order_no,
	ul.c_name,
	wo.create_stamp,
  woi.id,
  woi.gate_out_no,
  woi.sales_order_no,
  woi.item_code,
  woi.amount,
  woi.shelves,
  woi.cargo_bar_code
FROM
	wave_order wo
LEFT JOIN user_login ul ON wo.create_by = ul.id
LEFT JOIN wave_order_item woi ON wo.id = woi.order_id
where order_no=$P{order_no};]]>
